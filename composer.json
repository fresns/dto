{
    "name": "fresns/dto",
    "type": "library",
    "description": "Fresns data transfer object extension package for easy generation of data rules and inspection of transfer data. Works with Laravel projects.",
    "keywords": ["fresns", "dto", "laravel-dto", "data-transfer-object"],
    "license": "Apache-2.0",
    "homepage": "https://pm.fresns.org/dto/",
    "support": {
        "issues": "https://github.com/fresns/dto/issues",
        "source": "https://github.com/fresns/dto",
        "docs": "https://pm.fresns.org/dto/"
    },
    "authors": [
        {
            "name": "Jevan Tang",
            "email": "jevan@fresns.org",
            "homepage": "https://github.com/jevantang",
            "role": "Creator"
        },
        {
            "name": "mouyong",
            "email": "my24251325@gmail.com",
            "homepage": "https://github.com/mouyong",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "illuminate/support": "*",
        "illuminate/validation": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "friendsofphp/php-cs-fixer": "^3.52",
        "brainmaestro/composer-git-hooks": "^2.8",
        "phpunit/phpunit": "^9.6",
        "orchestra/testbench": "^6.43"
    },
    "autoload": {
        "psr-4": {
            "Fresns\\DTO\\": "src/"
        },
        "files": [
            "./src/Validate/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fresns\\DTO\\ServiceProvider"
            ]
        }
    }
}
